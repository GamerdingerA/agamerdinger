@article{ WOS:000490651300001,
Author = {Khorshidi, Hadi Akbarzadeh and Hassani-Mahmooei, Behrooz and Haffari,
   Gholamreza},
Title = {{An Interpretable Algorithm on Post-injury Health Service Utilization
   Patterns to Predict Injury Outcomes}},
Journal = {{JOURNAL OF OCCUPATIONAL REHABILITATION}},
